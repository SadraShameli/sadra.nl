{
    "name": "sadra.nl",
    "version": "0.1.0",
    "private": true,
    "type": "module",
    "scripts": {
        "build": "next build --turbo",
        "check": "next lint && tsc --noEmit",
        "db:generate": "drizzle-kit generate",
        "db:migrate": "drizzle-kit migrate",
        "db:push": "drizzle-kit push",
        "db:seed": "drizzle-kit generate && drizzle-kit migrate && bun --env-file .env ./src/server/db/seed.ts",
        "db:studio": "drizzle-kit studio",
        "dev": "next dev --turbo",
        "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
        "format:write": "prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
        "lint": "next lint",
        "lint:fix": "next lint --fix",
        "ncu": "bunx npm-check-updates --interactive --format group",
        "prepare": "husky",
        "preview": "next build --turbo && next start",
        "start": "next start",
        "stylelint:check": "stylelint '**/*.{css,scss}'",
        "stylelint:fix": "stylelint '**/*.{css,scss}' --fix",
        "typecheck": "tsc --noEmit"
    },
