{
    "name": "sadra.nl",
    "version": "0.1.0",
    "private": true,
    "type": "module",
    "scripts": {
        "build": "next build",
        "check": "next lint && tsc --noEmit",
        "db:generate": "drizzle-kit generate",
        "db:migrate": "drizzle-kit migrate",
        "db:push": "drizzle-kit push",
        "db:seed": "drizzle-kit generate && drizzle-kit migrate && bun --env-file .env ./src/server/db/seed.ts",
        "db:studio": "drizzle-kit studio",
        "dev": "next dev --turbo",
        "format": "prettier --check \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
        "format:write": "prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
        "lint": "next lint",
        "lint:fix": "next lint --fix",
        "ncu": "bunx npm-check-updates --interactive --format group",
        "prepare": "husky",
        "preview": "next build && next start",
        "prod": "next build && next start",
        "start": "next start",
        "stylelint": "stylelint '**/*.{css,scss}'",
        "stylelint:fix": "bun run stylelint -- --fix",
        "typecheck": "tsc --noEmit"
    },
    "dependencies": {
        "@radix-ui/react-avatar": "^1.1.3",
        "@radix-ui/react-dropdown-menu": "^2.1.6",
        "@radix-ui/react-popover": "^1.1.6",
        "@radix-ui/react-progress": "^1.1.2",
        "@radix-ui/react-scroll-area": "^1.2.3",
        "@radix-ui/react-separator": "^1.1.2",
        "@radix-ui/react-slider": "^1.2.3",
        "@radix-ui/react-slot": "^1.1.2",
        "@radix-ui/react-tabs": "^1.1.3",
        "@t3-oss/env-nextjs": "^0.12.0",
        "@tanstack/react-query": "^5.66.11",
        "@trpc/client": "^11.0.0-rc.708",
        "@trpc/react-query": "^11.0.0-rc.708",
        "@trpc/server": "^11.0.0-rc.708",
        "caniuse-lite": "^1.0.30001701",
        "class-variance-authority": "^0.7.1",
        "date-fns": "^4.1.0",
        "drizzle-orm": "^0.40.0",
        "framer-motion": "^12.4.7",
        "geist": "^1.3.1",
        "html-react-parser": "^5.2.2",
        "lucide-react": "^0.477.0",
        "next": "^15.2.0",
        "postgres": "^3.4.5",
        "react": "^19.0.0",
        "react-day-picker": "8.10.1",
        "react-dom": "^19.0.0",
        "recharts": "^2.15.1",
        "server-only": "^0.0.1",
        "sharp": "^0.33.5",
        "superjson": "^2.2.2",
        "zod": "^3.24.2"
    },
    "devDependencies": {
        "@eslint/eslintrc": "^3.3.0",
        "@eslint/js": "^9.21.0",
        "@tailwindcss/postcss": "^4.0.9",
        "@tailwindcss/typography": "^0.5.16",
        "@types/eslint": "^9.6.1",
        "@types/eslint__eslintrc": "^2.1.2",
        "@types/eslint__js": "^8.42.3",
        "@types/node": "^22.13.8",
        "@types/react": "^19.0.10",
        "@types/react-dom": "^19.0.4",
        "@typescript-eslint/eslint-plugin": "^8.25.0",
        "@typescript-eslint/parser": "^8.25.0",
        "@vercel/analytics": "^1.5.0",
        "@vercel/speed-insights": "^1.2.0",
        "cssnano": "^7.0.6",
        "cssnano-preset-default": "^7.0.6",
        "drizzle-kit": "^0.30.5",
        "eslint": "^9.21.0",
        "eslint-config-next": "^15.2.0",
        "eslint-plugin-drizzle": "^0.2.3",
        "husky": "^9.1.7",
        "postcss": "^8.5.3",
        "prettier": "^3.5.2",
        "prettier-plugin-tailwindcss": "^0.6.11",
        "stylelint": "^16.15.0",
        "stylelint-config-recommended-scss": "^14.1.0",
        "stylelint-order": "^6.0.4",
        "tailwind-merge": "^3.0.2",
        "tailwindcss": "^4.0.9",
        "tailwindcss-animate": "^1.0.7",
        "typescript": "^5.8.2"
    },
    "trustedDependencies": [
        "@vercel/speed-insights"
    ]
