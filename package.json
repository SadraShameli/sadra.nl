{
    "name": "sadra.nl",
    "version": "0.1.0",
    "private": true,
    "type": "module",
    "scripts": {
        "build": "next build",
        "check": "next lint && tsc --noEmit",
        "db:generate": "drizzle-kit generate",
        "db:migrate": "drizzle-kit migrate",
        "db:push": "drizzle-kit push",
        "db:seed": "drizzle-kit generate && drizzle-kit migrate && bun --env-file .env ./src/server/db/seed.ts",
        "db:studio": "drizzle-kit studio",
        "dev": "next dev --turbo",
        "format": "prettier --check \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
        "format:write": "prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
        "ncu": "bunx npm-check-updates --interactive --format group",
        "prepare": "husky",
        "preview": "next build && next start",
        "start": "next start",
        "stylelint": "stylelint '**/*.{css,scss}'",
        "stylelint:fix": "stylelint '**/*.{css,scss}' -- --fix --quiet",
        "typecheck": "tsc --noEmit"
    },
    "dependencies": {
        "@radix-ui/react-avatar": "^1.1.11",
        "@radix-ui/react-dropdown-menu": "^2.1.16",
        "@radix-ui/react-popover": "^1.1.15",
        "@radix-ui/react-progress": "^1.1.8",
        "@radix-ui/react-scroll-area": "^1.2.10",
        "@radix-ui/react-separator": "^1.1.8",
        "@radix-ui/react-slider": "^1.3.6",
        "@radix-ui/react-slot": "^1.2.4",
        "@radix-ui/react-tabs": "^1.1.13",
        "@t3-oss/env-nextjs": "^0.13.8",
        "@tanstack/react-query": "^5.90.11",
        "@trpc/client": "^11.7.2",
        "@trpc/react-query": "^11.7.2",
        "@trpc/server": "^11.7.2",
        "caniuse-lite": "^1.0.30001759",
        "class-variance-authority": "^0.7.1",
        "date-fns": "^4.1.0",
        "drizzle-orm": "^0.44.7",
        "framer-motion": "^12.23.25",
        "geist": "^1.5.1",
        "html-react-parser": "^5.2.10",
        "lucide-react": "^0.555.0",
        "next": "^16.0.7",
        "postgres": "^3.4.7",
        "react": "^19.2.1",
        "react-day-picker": "^9.11.3",
        "react-dom": "^19.2.1",
        "recharts": "2.15.4",
        "server-only": "^0.0.1",
        "sharp": "^0.34.5",
        "superjson": "^2.2.6",
        "zod": "^4.1.13"
    },
    "devDependencies": {
        "@eslint/eslintrc": "^3.3.3",
        "@eslint/js": "^9.39.1",
        "@tailwindcss/postcss": "^4.1.17",
        "@tailwindcss/typography": "^0.5.19",
        "@types/eslint": "^9.6.1",
        "@types/node": "^24.10.1",
        "@types/react": "^19.2.7",
        "@types/react-dom": "^19.2.3",
        "@typescript-eslint/eslint-plugin": "^8.48.1",
        "@typescript-eslint/parser": "^8.48.1",
        "@vercel/analytics": "^1.6.1",
        "@vercel/speed-insights": "^1.3.1",
        "cssnano": "^7.1.2",
        "cssnano-preset-default": "^7.0.10",
        "drizzle-kit": "^0.31.7",
        "eslint": "^9.39.1",
        "eslint-config-next": "^16.0.7",
        "eslint-plugin-drizzle": "^0.2.3",
        "husky": "^9.1.7",
        "postcss": "^8.5.6",
        "prettier": "^3.7.4",
