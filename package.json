        "@trpc/server": "^11.1.2",
        "caniuse-lite": "^1.0.30001717",
        "class-variance-authority": "^0.7.1",
        "date-fns": "^4.1.0",
        "drizzle-orm": "^0.43.1",
        "framer-motion": "^12.10.4",
        "geist": "^1.4.2",
        "html-react-parser": "^5.2.3",
        "lucide-react": "^0.508.0",
